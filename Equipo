  bomba: BOMBA;
  kitMangueras: KITMANGUERAS;

  constructor(motor: MOTOR, bomba: BOMBA, kitMangueras: KITMANGUERAS) {
    this.motor = motor;
    this.bomba = bomba;
    this.kitMangueras = kitMangueras;
  }
}

